#include <stdio.h>
#include<conio.h>
int main() 
{ 
	

	int n, re, i, j, k; 
	n = 5;
	re = 3;
	clrscr;
	int alloc[5][3] = { { 0, 1, 1 }, 
						{ 2, 0, 1 }, 
						{ 1, 2, 1 }, 
						{ 2, 2, 2 }, 
						{ 0, 1, 3 } }; 

	int max[5][3] = { { 6, 5, 3 }, 
					{ 2, 1, 2 }, 
					{ 7, 0, 2 }, 
					{ 2, 1, 3 }, 
					{ 5, 2, 3 } }; 

	int avail[3] = { 4, 3, 4};

	int f[n], ans[n], ind = 0; 
	for (k = 0; k < n; k++) { 
		f[k] = 0; 
	} 
	int need[n][re]; 
	for (i = 0; i < n; i++) { 
		for (j = 0; j < re; j++) 
			need[i][j] = max[i][j] - alloc[i][j]; 
	} 
	int y = 0; 
	for (k = 0; k < 5; k++) { 
		for (i = 0; i < n; i++) { 
			if (f[i] == 0) { 

				int flag = 0; 
				for (j = 0; j < re; j++) { 
					if (need[i][j] > avail[j]){ 
						flag = 1; 
						break; 
					} 
				} 

				if (flag == 0) { 
					ans[ind++] = i; 
					for (y = 0; y < re; y++) 
						avail[y] += alloc[i][y]; 
					f[i] = 1; 
				} 
			} 
		} 
	} 

	printf("Th SAFE Sequence is as follows\n"); 
	for (i = 0; i < n - 1; i++) 
		printf(" P%d ->", ans[i]); 
	printf(" P%d", ans[n - 1]); 

	getch(); 

} 
